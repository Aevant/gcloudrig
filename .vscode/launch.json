{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "gcloudrig setup",
            "program": "${workspaceFolder}/cli.js",
            "env": {
                "NODE_ENV": "development"
            },
            "args": [
                "serve"
            ],
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}